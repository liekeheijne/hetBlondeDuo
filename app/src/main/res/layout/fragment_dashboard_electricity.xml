<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="20dp"
    tools:context="nl.lucmulder.watt.app.dashboard_power">

    <!-- TODO: Update blank fragment layout -->
    <nl.lucmulder.watt.lib.CircularProgressBar
        android:id="@+id/circularProgressElectric"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/electricView"
        android:layout_below="@+id/circularProgressElectric"
        android:textAlignment="center"
        android:textSize="30sp"/>

</RelativeLayout>